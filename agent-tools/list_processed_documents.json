{
  "type": "function",
  "function": {
    "name": "list_processed_documents",
    "description": "List all processed documents with optional filtering by document type, AFV status, or discharge eligibility. Supports pagination and sorting.",
    "parameters": {
      "type": "object",
      "properties": {
        "page": {
          "type": "number",
          "description": "Page number for pagination (default: 1)"
        },
        "limit": {
          "type": "number",
          "description": "Number of results per page (default: 10, max: 100)"
        },
        "documentType": {
          "type": "string",
          "description": "Filter by document type",
          "enum": [
            "affidavit",
            "notice",
            "commercial_instrument",
            "promissory_note",
            "bill_of_exchange",
            "invoice",
            "contract",
            "correspondence",
            "court_filing",
            "other"
          ]
        },
        "afvFound": {
          "type": "boolean",
          "description": "Filter by whether AFV notation was found"
        },
        "dischargeEligible": {
          "type": "boolean",
          "description": "Filter by discharge eligibility status"
        }
      }
    },
    "returns": {
      "type": "object",
      "description": "List of processed documents with pagination info"
    },
    "api_endpoint": "GET /api/document-intelligence"
  }
}
