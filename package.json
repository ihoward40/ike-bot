{
  "name": "ike-bot",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "router:dev": "ts-node-dev --respawn --transpile-only src/router-server.ts",
    "router:start": "node dist/router-server.js",
    "router:test": "node test/sintraprime-router.test.js",
    "notion:test": "jest test/notion-case-linker.test.js",
    "template:test": "jest test/template-registry.test.js",
    "test": "jest",
    "db:init": "supabase db execute --file supabase/migrations/00001_init_schema.sql",
    "db:seed": "supabase db execute --file supabase/migrations/00002_seed_data.sql",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@supabase/supabase-js": "^2.84.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.77.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "stripe": "^20.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.5.1",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
